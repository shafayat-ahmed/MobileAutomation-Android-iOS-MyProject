            <class name="Entertainment.EntertainmentPageTest"/>
            <class name="Fashion.FashionPageTest"/>
            <class name="LatestNews.LatestNewsPageTest"/>
            <class name="LivingPage.LivingPageTest"/>
            <class name="MediaPage.MediaPageTest"/>
            <class name="MetroPage.MetroPageTest"/>
            <class name="NewsPage.NewsPageTest"/>
            <class name="Notification.NotificationsPageTest"/>
            <class name="opinionPage.Opinion"/>
            <class name="PageSix.PageSixPageTest"/>
            <class name="PhotoPage.PhotosPageTest"/>
            <class name="RealEstatePage.RealEstatePageTest"/>
            <class name="SearchPage.SearchPageTest"/>
            <class name="settingsPage.Settings"/>
            <class name="Tech.TestTechPage"/>
            <class name="TopNews.TopNewsTest"/>
            <class name="Videos.TestVideos"/>
            <class name="Tech.TestTechPage"/>
        </classes>
